{
    "components": [{
        "type": "flex",
        "name": "layout",
        "class": "header",
        "defaultValue": "test",
        "children": [{
                "name": "business._extension.soleProprietorship",
                "type": "radiobuttonwithlist",
                "label": "Is the business you're filling out this application for a sole proprietorship?",
                "placeHolder": "Please enter firstname",
                "children": [],
                "required": true,
                "checklist": [
                    "Pawn Broker",
                    "Currency Exchanger",
                    "Check Casher"
                ],
                "labelValues": [{
                        "value": "true",
                        "label": "Yes"
                    },
                    {
                        "value": "false",
                        "label": "No"
                    }
                ]
            },
            {
                "name": "owners.0.firstName",
                "type": "text",
                "required": true,
                "label": "First Name",
                "placeHolder": "Please enter first name",
                "children": [],
                "validation": {
                    "required": { "value": true, "message": "This is required" },
                    "maxLength": { "value": 5, "message": "This cannot exceed 5 characters" }
                }
            },
            {
                "name": "owners.0.lastName",
                "type": "text",
                "required": true,
                "label": "Last Name",
                "placeHolder": "Please enter last name",
                "children": [],
                "validation": {
                    "required": { "value": true, "message": "This is required" }
                }
            },
            {
                "name": "owners.0.email",
                "type": "text",
                "required": true,
                "label": "Email",
                "placeHolder": "Please enter email",
                "children": [],
                "validation": {
                    "required": { "value": true, "message": "This is required" },
                    "pattern": {
                        "regex": "\/^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,}$\/i",
                        "message": "Invalid email address"
                    },
                    "customValidate": "email"
                }
            },
            {
                "type": "flex",
                "name": "layout",
                "class": "header",
                "style": {
                    "flexFlow": "row wrap",
                    "margin": "0",
                    "border": "0",
                    "padding": "0"
                },
                "defaultValue": "test",
                "children": [{
                        "type": "text",
                        "name": "owners.0.address",
                        "required": true,
                        "label": "Street",
                        "placeHolder": "test placeholder 2",
                        "children": []
                    },
                    {
                        "type": "text",
                        "name": "owners.0.county",
                        "required": true,
                        "label": "County",
                        "placeHolder": "test placeholder 2",
                        "children": []
                    },
                    {
                        "type": "dropdown",
                        "name": "owners.0.state",
                        "required": true,
                        "label": "State",
                        "placeHolder": "Select State",
                        "labelValues": [
                            { "label": "AL", "value": "AL" },
                            { "label": "AK", "value": "AK" },
                            { "label": "MD", "value": "MD" },
                            { "label": "VA", "value": "VA" }
                        ],
                        "children": []
                    }
                ]
            },
            {
                "type": "numeric",
                "required": true,
                "format": "##-#######",
                "name": "business.taxId",
                "label": "Business Tax ID (EIN)",
                "placeHolder": "##-#######",
                "children": [],
                "validation": {
                    "required": { "value": true, "message": "Please enter Business Tax ID" }
                },
                "rule": {
                    "variable": "business._extension.soleProprietorship",
                    "logic": {
                        "==": [{
                                "var": "business._extension.soleProprietorship"
                            },
                            "true"
                        ]
                    }
                }
            },
            {
                "type": "ownershippercentage",
                "name": "ownershippercentage",
                "children": []
            },
            {
                "type": "dynamic",
                "name": "coOwners",
                "label": "Co-Owner",
                "children": [{
                    "type": "flex",
                    "name": "layout2",
                    "children": [{
                            "name": "",
                            "dynamicName": "coOwners.[index].firstName",
                            "type": "text",
                            "required": true,
                            "label": "First Name",
                            "placeHolder": "Please enter first name",
                            "children": []
                        },
                        {
                            "name": "",
                            "dynamicName": "coOwners.[index].lastName",
                            "type": "text",
                            "required": true,
                            "label": "Last Name",
                            "placeHolder": "Please enter last name",
                            "children": []
                        },
                        {
                            "name": "",
                            "dynamicName": "coOwners.[index].ownershipPercentage",
                            "type": "text",
                            "required": true,
                            "label": "Ownership %",
                            "placeHolder": "Please enter ownership %",
                            "children": [],
                            "validation": {
                                "max": { "value": 100, "message": "Cannot exceed 100" }
                            }
                        }
                    ]
                }]
            },
            {
                "type": "submit",
                "name": "testSubmit",
                "children": []
            }
        ]
    }]
}